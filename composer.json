{
    "name": "phpextra/unknown",
    "description": "Unknown type for PHP",
    "keywords": ["unknown type", "object wrapper"],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/phpextra/unknown",
    "support": {
        "issues": "https://github.com/phpextra/unknown/issues",
        "source": "https://github.com/phpextra/unknown"
    },
    "authors": [
        {
            "name": "Jacek Kobus",
            "email": "kobus.jacek@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "PHPExtra\\Type\\Unknown": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "phpextra/collection": "dev-master as 1.0.0",
        "phpextra/enum": "~1.0",
        "phpextra/sorter": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "archive": {
        "exclude": ["*", ".*", "!/src", "!/README.md", "!/LICENSE.md"]
    }
}
